{
  "lock": false,
  "tasks": {
    "stop": "pm2 stop josefabio",
    "logs": "pm2 logs josefabio",
    "delete": "pm2 delete josefabio",
    "reload": "deno task build && pm2 reload josefabio && pm2 logs josefabio",
    "start": "deno task build && DENO_DEPLOYMENT_ID=\"$(git rev-parse HEAD)\" pm2 start --interpreter='deno' --interpreter-args='run --allow-import --allow-sys --allow-net --allow-read --allow-env --allow-run --allow-write' --name josefabio main.ts && pm2 logs josefabio",
    "check": "deno check --allow-import main.ts dev.ts",
    "dev": "deno run -A --watch=static/,routes/ dev.ts",
    "watch": "deno run -A --watch=static/,routes/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts",
    "update_deps": "deno outdated",
    "update_deps_now": "deno outdated --update --latest",
    "update": "deno run -A -r https://fresh.deno.dev/update ."
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "@preact-icons/ai": "jsr:@preact-icons/ai@^1.0.13",
    "@preact-icons/bi": "jsr:@preact-icons/bi@^1.0.13",
    "@preact-icons/bs": "jsr:@preact-icons/bs@^1.0.13",
    "@preact-icons/cg": "jsr:@preact-icons/cg@^1.0.13",
